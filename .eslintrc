{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "indent": [2, 2, { "SwitchCase": 1 }],
    "linebreak-style": [1, "unix"],
    "semi": [0, "always"],
    "no-console": 0,
    "rest-spread-spacing": [2, "never"]
  }
}
